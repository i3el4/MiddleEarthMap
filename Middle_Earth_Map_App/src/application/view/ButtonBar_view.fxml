<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.HBox?>

<fx:root fx:id="buttonbar_root_pane" spacing="20.0"
	alignment="CENTER" maxHeight="-Infinity" maxWidth="-Infinity"
	minHeight="-Infinity" minWidth="-Infinity" prefHeight="50.0"
	prefWidth="300.0" styleClass="button-bar"
	stylesheets="@application.css" type="javafx.scene.layout.HBox"
	xmlns="http://javafx.com/javafx/8"
	xmlns:fx="http://javafx.com/fxml/1">
	<children>
		<Button defaultButton="true" graphicTextGap="200.0"
			mnemonicParsing="false" text="Add" wrapText="true">
			<opaqueInsets>
				<Insets />
			</opaqueInsets>
		</Button>
		<Button graphicTextGap="200.0" mnemonicParsing="false"
			text="Edit" wrapText="true" />
		<Button graphicTextGap="200.0" mnemonicParsing="false"
			text="Delete" wrapText="true" />
	</children>
</fx:root>
